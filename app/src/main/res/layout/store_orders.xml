<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <EditText
                android:layout_width="match_parent"
                android:layout_height="267dp"
                android:gravity="start|top"
                android:ems="10"
                android:id="@+id/editTextTextMultiLine2" android:inputType="none" android:editable="false"
                android:autofillHints="" tools:ignore="Deprecated,LabelFor"/>
        <Spinner
                android:layout_width="match_parent"
                android:layout_height="40dp" android:id="@+id/spinner2"/>
        <Button
                android:text="@string/removeOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button10" android:layout_marginHorizontal="10dp"
        />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>